extends layout

block content
  h3 Login with Github
  a.btn.btn-primary(href="/auth/github") Login
  form.form-inline(action="/login", method="post")
    label 
      h3 email
      input.form-control(type='text' name='email')
    label 
      h3 password
      input.form-control(type='password' name='password')
    input.btn.btn-default(type='submit' value='log in')
      
